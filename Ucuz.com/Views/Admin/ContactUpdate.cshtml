@model DTO.DTOEntity.ContactDTO
@{
    ViewData["Title"] = "ContactUpdate";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<div class="grids">
    <div class="progressbar-heading grids-heading">
        <h2>Input Forms</h2>
    </div>
    <div class="panel panel-widget forms-panel">
        <div class="forms">
            <div class="form-grids widget-shadow" data-example-id="basic-forms">
                <div class="form-title">
                    <h4>Update :</h4>
                </div>
                <div class="form-body">
                    <form method="post" enctype="multipart/form-data">
                        <div class="form-group">
                            <label for="exampleInputEmail1">Address</label>
                            <input type="text" asp-for="Address" class="form-control" id="exampleInputEmail1" placeholder="Address">
                        </div>
                        <div class="form-group">
                            <label for="exampleInputPassword1">Phone</label>
                            <input type="text" asp-for="Phone" class="form-control" id="exampleInputPassword1" placeholder="Phone">
                        </div>

                        <div class="form-group">
                            <label for="exampleInputPassword1">Gmail</label>
                            <input type="text" asp-for="Gmail" class="form-control" id="exampleInputPassword1" placeholder="Gmail">
                        </div>

                        <div class="form-group">
                            <label for="exampleInputPassword1">Map</label>
                            <input type="text" asp-for="Map" class="form-control" id="exampleInputPassword1" placeholder="Map">
                        </div>

                        <button type="submit" class="btn btn-default w3ls-button">Submit</button>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>


