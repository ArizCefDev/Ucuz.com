@model DTO.DTOEntity.AboutDTO

@{
    ViewData["Title"] = "AboutUpdate";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<div class="grids">
    <div class="progressbar-heading grids-heading">
        <h2>Input Forms</h2>
    </div>
    <div class="panel panel-widget forms-panel">
        <div class="forms">
            <div class="form-grids widget-shadow" data-example-id="basic-forms">
                <div class="form-title">
                    <h4>About Update :</h4>
                </div>
                <div class="form-body">
                    <form method="post" enctype="multipart/form-data">
                        <div class="form-group">
                            <label for="exampleInputEmail1">Photo 1</label>
                            <input type="text" asp-for="Image1" class="form-control" id="exampleInputEmail1" placeholder="Photo">
                        </div>
                         <div class="form-group">
                            <label for="exampleInputEmail1">Photo 2</label>
                            <input type="text" asp-for="Image2" class="form-control" id="exampleInputEmail1" placeholder="Photo">
                        </div>
                        <div class="form-group">
                            <label for="exampleInputEmail1">Photo 3</label>
                            <input type="text" asp-for="Image3" class="form-control" id="exampleInputEmail1" placeholder="Photo">
                        </div>
                        <div class="form-group">
                            <label for="exampleInputEmail1">Photo 4</label>
                            <input type="text" asp-for="Image4" class="form-control" id="exampleInputEmail1" placeholder="Photo">
                        </div>
                        <div class="form-group">
                            <label for="exampleInputPassword1">Title</label>
                            <input type="text" asp-for="Title" class="form-control" id="exampleInputPassword1" placeholder="Title">
                        </div>

                        <div class="form-group">
                            <label for="exampleInputPassword1">Description</label>
                            <input type="text" asp-for="Description" class="form-control" id="exampleInputPassword1" placeholder="Description">
                        </div>
                        <button type="submit" class="btn btn-default w3ls-button">Submit</button>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>

